<p id="notice"><%= notice %></p>

<h1>Listing Jeans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jeans.each do |jean| %>
      <tr>
        <td><%= link_to 'Show', jean %></td>
        <td><%= link_to 'Edit', edit_jean_path(jean) %></td>
        <td><%= link_to 'Destroy', jean, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jean', new_jean_path %>
